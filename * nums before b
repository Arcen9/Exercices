import java.util.Scanner;

class MyProgram{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        long a = sc.nextInt();
        long b = sc.nextInt();
        long sum = 1;
        while(a <= b){
            sum *= a;
            a++;
        }
        System.out.println(sum);
    }
}
