import java.util.Scanner;

class MyProgram{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int temp = n;
        int sum = 0;
        for (int i = 1; i <= temp; i++) {
            n = sc.nextInt();
            sum += n;
        }
        System.out.println(sum);
    }
}
