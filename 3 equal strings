import java.util.Scanner;

class MyProgram{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.nextLine();
        String b = sc.nextLine();
        String c = sc.nextLine();
        if(a.equals(b) && a.equals(c)){
            System.out.print("Yes");
        }else{
            System.out.print("No");
        }
    }
}
