import java.util.Scanner;

class MyProgram{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int temp = n;
        int count = 0;
        for (int i = 1; i <= temp; i++) {
            n = sc.nextInt();
            if(n % 2 == 0) count++;
        }
        System.out.println(count);
    }
}
