import java.util.Scanner;

class MyProgram{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int sum = 0;
        while(a % 7 != 0){
            if(a % 10 == 7) sum += a;
            a = sc.nextInt();
        }
        System.out.println(sum);
    }
}
