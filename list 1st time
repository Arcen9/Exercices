import java.util.ArrayList;
import java.util.Scanner;

class MyProgram {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        ArrayList<Integer> list = new ArrayList<>();
        list.add(1);
        for (int i = 1; i < n + 1; i++) {
            list.add(0);
        }
        list.add(1);
        for (int i = 0; i <  n + 2; i++) {
            System.out.print(list.get(i) + " ");
        }
    }
}
