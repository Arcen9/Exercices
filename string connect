import java.util.Scanner;

class MyProgram {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        sc.nextLine();
        String[] str = new String[n];
        for (int i = 0; i < n; i++) {
            str[i] = sc.nextLine();
        }
        String def = sc.nextLine();
        for (int i = 0; i < n; i++) {
            System.out.print(str[i]);
            if(i != n - 1) System.out.print(def);
        }
    }
}
